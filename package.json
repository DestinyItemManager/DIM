{
  "name": "dim",
  "version": "7.6.0",
  "description": "An item manager for Destiny.",
  "main": "app/index.html",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --verbose",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint src --ext .js,.ts,.tsx",
    "lint:prettier": "prettier src/**/*.{js,ts,tsx,scss,html} --check",
    "lint:stylelint": "stylelint src/**/*.scss",
    "lintcached": "run-p lintcached:*",
    "lintcached:eslint": "yarn lint:eslint --cache --cache-location .eslintcache --cache-strategy content",
    "fix": "run-s fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:json": "jsonsort src/locale",
    "fix:stylelint": "yarn lint:stylelint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "bundle": "webpack --config ./config/webpack.js",
    "build:release": "yarn -s bundle --env=release --node-env=production",
    "build:beta": "yarn -s bundle --env=beta --node-env=production",
    "build:dev": "yarn -s bundle --env=dev",
    "i18n": "i18next-scanner",
    "watch": "yarn -s build:dev --watch",
    "start": "webpack serve --config ./config/webpack.js --env=dev",
    "syntax": "find dist -name \"*.js\" | xargs -n 1 node --check",
    "install": "git submodule update --init --recursive",
    "dead-code": "ts-unused-exports tsconfig.json --allowUnusedTypes --allowUnusedEnums --showLineNumber | grep -v cssExports",
    "bcu": "run-s bcu:*",
    "bcu:build": "node src/build-browsercheck-utils.js",
    "bcu:pretty": "prettier --write src/browsercheck-utils.js",
    "test-and-lint": "run-p test lintcached"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DestinyItemManager/DIM.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DestinyItemManager/DIM/issues"
  },
  "homepage": "https://destinyitemmanager.com",
  "browserslist": [
    "Chrome >= 79",
    "last 2 ChromeAndroid versions",
