{
  "compilerOptions": {
    "strict": true,
    "useUnknownInCatchVariables": false,
    "esModuleInterop": true,
    "target": "esnext",
    "module": "esnext",
    "lib": ["webworker", "esnext"],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "sourceMap": true,
    "allowJs": false,
    "baseUrl": ".",
    "paths": {
      "app/*": ["../../*"],
      "data/*": ["../../../data/*"]
    }
  },
  "include": ["./*", "../../../global.d.ts"]
}
