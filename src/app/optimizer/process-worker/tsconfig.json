{
  "compilerOptions": {
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "noUnusedParameters": true,
    "target": "esnext",
    "module": "esnext",
    "lib": ["webworker", "esnext"],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowJs": false,
    "baseUrl": "."
  }
}
