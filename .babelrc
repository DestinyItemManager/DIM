{
  "presets": [
    ["env", {
      "modules": false,
      "useBuiltIns": true,
      "targets": {
        "uglify": true,
        "browsers": [
          "last 2 Chrome versions",
          "last 2 ChromeAndroid versions",
          "last 2 Firefox versions",
          "last 1 Safari versions",
          "last 1 iOS versions",
          "last 2 Edge versions"
        ]
      }
    }]
  ],
  "plugins": ["angularjs-annotate", "syntax-dynamic-import"]
}
