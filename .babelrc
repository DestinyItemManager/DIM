{
  "presets": [
    ["env", {
      "modules": false,
      "useBuiltIns": true,
      "targets": {
        "uglify": true,
        "browsers": [
          "Chrome >= 51",
          "last 2 ChromeAndroid versions",
          "last 2 Firefox versions",
          "last 2 Safari versions",
          "iOS >= 10",
          "last 2 Edge versions",
          "last 2 Opera versions"
        ]
      }
    }]
  ],
  "plugins": [
    "angularjs-annotate",
    "syntax-dynamic-import",
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ]
}
