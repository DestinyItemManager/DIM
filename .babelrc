{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "last 2 Chrome versions",
          "last 2 ChromeAndroid versions",
          "last 2 Firefox versions",
          "last 1 Safari versions",
          "last 1 iOS versions",
          "last 2 Edge versions"
        ]
      },
      "modules": false,
      "loose": true,
      "debug": true
    }]
  ],
  "plugins": ["angularjs-annotate"]
}
