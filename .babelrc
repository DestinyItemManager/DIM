{
  "presets": [
    ["env", {
      "modules": false,
      "useBuiltIns": true,
      "targets": {
        "uglify": true,
        "browsers": [
          "Chrome >= 58",
          "ChromeAndroid >= 59",
          "Firefox >= 53",
          "Safari >= 10",
          "iOS >= 10",
          "Edge >= 14"
        ]
      }
    }]
  ],
  "plugins": [
    "angularjs-annotate",
    "syntax-dynamic-import",
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ]
}
