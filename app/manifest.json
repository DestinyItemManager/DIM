{
  "manifest_version": 2,
  "name": "DIM's Old Dev Path",
  "version": "3.16.0",
  "description": "Outdated path for DIM.",
  "icons": {
    "16": "icon16.png"
  },
  "short_name": "DIM-Old",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "options_ui": {
    "page": "index.html",
    "chrome_style": false,
    "open_in_tab": true
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png"
    }
  },
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
